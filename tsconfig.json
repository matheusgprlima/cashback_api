    {
  "compilerOptions": {                   
    "target": "es2017",                          
    "module": "commonjs",                     
    "lib": ["ES6"],                           
    "allowJs": true,                       
    "outDir": "./dist",                      
    "rootDir": "../cashback_api",                      
    "removeComments": true,                
